#include <stdio.h>
#include <math.h>
int main(){
    int n,i,bin,decimal;
    i=0;
    bin = 0;
    scanf("%d",&n);
    decimal = n;
    while(n>=1){
        if(n%2==0){
            n=n/2;
        }
        else{
            n = (n-1)/2;
            bin = bin + pow(10,i);
        }
        i++;
    }
    printf("Decimal:%d Bin√°rio:%d",decimal,bin);
}
